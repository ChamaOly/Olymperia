<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

     <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#E0E0E0"
                android:endColor="#B0BEC5"
                android:angle="45" />
            <corners android:radius="16dp" />
