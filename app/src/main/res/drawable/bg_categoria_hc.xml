<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#80D8FF"
                android:endColor="#01579B"
                android:angle="45" />
            <corners android:radius="16dp" />
